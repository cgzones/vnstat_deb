diff --git a/src/dbcache.c b/src/dbcache.c
index a3b717f..a4329d6 100644
--- a/src/dbcache.c
+++ b/src/dbcache.c
@@ -330,19 +330,19 @@ uint32_t dbcheck(uint32_t dbhash, int *forcesave)
 	return newhash;
 }
 
-uint32_t simplehash(const char *data, int len)
