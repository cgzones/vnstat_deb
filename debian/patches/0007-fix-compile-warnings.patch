 {
 	uint32_t hash = len;
 
-	if (len <= 0 || data == NULL) {
+	if (len <= 0 || input == NULL) {
 		return 0;
 	}
 
 	for (len--; len >= 0; len--) {
 		if (len > 0) {
-			hash += (int)data[len] * len;
+			hash += (int)input[len] * len;
 		} else {
-			hash += (int)data[len];
+			hash += (int)input[len];
 		}
 	}
 
